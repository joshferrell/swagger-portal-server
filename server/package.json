{
  "name": "swagger-server",
  "version": "1.0.0",
  "description": "api for uploading documenation for swagger",
  "main": "lib/server.js",
  "repository": "https://github.com/joshferrell/swagger-portal-server/tree/master/server",
  "author": {
    "name": "Josh Ferrell",
    "url": "https://github.com/joshferrell/swagger-portal-server"
  },
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=local nodemon src/server.js --exec babel-node | bunyan",
    "lint": "eslint src/**/**.js",
    "build": "babel src -d build --copy-files",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "deploy:local": "docker-compose up -d --build",
    "deploy:clean": "docker-compose down --rmi local"
  },
  "dependencies": {
    "boom": "^6.0.0",
    "bunyan": "^1.8.12",
    "common-tags": "^1.4.0",
    "dotenv-safe": "^4.0.4",
    "glue": "^4.2.1",
    "good": "^7.3.0",
    "good-bunyan": "^1.3.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.6.2",
    "hapi-swagger": "^8.0.0",
    "inert": "^4.2.1",
    "joi": "^13.0.1",
    "moment": "^2.19.1",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "pkginfo": "^0.4.1",
    "ramda": "^0.25.0",
    "sequelize": "^4.19.0",
    "tv": "^6.1.0",
    "vision": "4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1"
  }
}
